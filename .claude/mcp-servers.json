{
  "github": {
    "command": "github-mcp-server",
    "args": ["--repo", "CVO-TreeAi/TreeAI-TreeShop"],
    "env": {
      "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
    }
  },
  "gitmcp": {
    "command": "curl",
    "args": [
      "-X", "POST",
      "https://gitmcp.io/CVO-TreeAi/TreeAI-TreeShop",
      "-H", "Content-Type: application/json"
    ]
  }
}